  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/node": "^18.15.9",
    "@vill-v/bumpp": "^0.2.2",
    "@vill-v/eslint-config": "^0.0.13",
    "@vill-v/prettier-config": "^0.0.13",
    "@vill-v/tsconfig": "^0.0.13",
    "@vrx/core": "workspace:*",
    "eslint": "^8.36.0",
    "fast-glob": "^3.2.12",
    "fs-extra": "^11.1.1",
    "gray-matter": "^4.0.3",
    "prettier": "^2.8.7",
    "taze": "^0.9.1",
    "tsup": "^6.7.0",
    "turbo": "^1.8.5",
    "typescript": "^5.0.2",
    "unocss": "^0.50.6",
    "vitepress": "^1.0.0-alpha.61",
    "void-fs": "^0.0.1-beta.8",
    "vue": "^3.2.47"
  },
  "pnpm": {
    "overrides": {
      "vue-demi": "0.13.11"
    },
    "packageExtensions": {
      "vue-demi": {
        "dependencies": {
          "vue": "^3.2.47"
        },
        "peerDependencies": {
          "vue": "^3.2.47"
        }
      }
