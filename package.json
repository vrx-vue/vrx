    "@vill-v/prettier-config": "^0.2.2",
    "@vill-v/tsconfig": "^0.2.2",
    "@vite-pwa/assets-generator": "^0.0.10",
    "@vite-pwa/vitepress": "^0.2.1",
    "@vrx/core": "workspace:*",
    "eslint": "^8.49.0",
    "fast-glob": "^3.3.1",
    "gray-matter": "^4.0.3",
    "prettier": "^3.0.3",
    "taze": "^0.11.2",
    "tsup": "^7.2.0",
    "turbo": "^1.10.13",
    "typescript": "^5.2.2",
    "unocss": "^0.55.7",
